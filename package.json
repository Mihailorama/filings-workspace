{
  "name": "@cfl/filings-app",
  "version": "1.2.1-local",
  "cfl": {
    "group": {
      "name": "labs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoreFiling/filings-app.git"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/CoreFiling/filings-app",
  "scripts": {
    "prepublishOnly": "rm -rf www && npm run compile",
    "compile": "npm run compile:webpack",
    "compile:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors --bail",
    "test": "npm run test:tslint && npm run test:karma",
    "test:karma": "karma start karma.conf.js",
    "test:tslint": "tslint -t stylish -c tslint.json --project tsconfig.json",
    "dev:server": "webpack-dev-server --config webpack.cfl.config.js --progress --colors",
    "dev:karma": "npm run test:karma -- --single-run=false --debug --auto-watch",
    "set-build-version": "npm version \"$(ci-build-version -f package.json)\"",
    "test-debug": "npm run dev:karma",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prestart": "npm run compile:webpack",
    "start": "simple-platform-server -d www -a filings-app -p 8080"
  },
  "config": {
    "devserver-https": true,
    "devserver-host": "localhost",
    "devserver-port": 9091,
    "api-proxy": "https://labs-api.cfl.io/",
    "external-app-proxy": "https://platform.cfl.io/"
  },
  "files": [
    "www"
  ],
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "dependencies": {
    "@cfl/simple-platform-server": "^2.1.0",
    "@types/react-helmet": "^5.0.3",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@cfl/filing-statistics-service": "^1.0.0",
    "@cfl/react-components": "^1.4.2",
    "@cfl/table-rendering-service": "^1.4.3",
    "@cfl/table-viewer": "^0.2.0",
    "@cfl/tslint-config": "^0.6.1",
    "@janslow/ci-build-version": "^0.1.1",
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "@types/classnames": "^2.2.3",
    "@types/cookie": "^0.3.1",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jasmine": "^2.5.47",
    "@types/react": "15",
    "@types/react-bootstrap": "^0.31.2",
    "@types/react-dom": "^0.14.23",
    "@types/react-dropzone": "^4.1.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^5.0.7",
    "@types/react-router-dom": "^4.2.1",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__react": "^3.0.5",
    "@types/uri-templates": "^0.1.28",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.0",
    "classnames": "^2.2.5",
    "cookie": "^0.3.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "human-size": "^1.1.0",
    "imports-loader": "^0.7.1",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jasmine-expect": "^3.7.0",
    "jasmine-immutablejs-matchers": "^0.1.2",
    "jasmine-reporters": "^2.2.1",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "license-webpack-plugin": "^0.4.3",
    "normalize.css": "^6.0.0",
    "react": "^15.5.0",
    "react-addons-test-utils": "^15.5.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.5.0",
