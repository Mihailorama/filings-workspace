    "skin-deep": "^1.0.0-alpha2",
    "source-map-support": "^0.4.14",
    "style-loader": "^0.16.1",
    "title-case": "^2.1.1",
    "transfer-webpack-plugin": "^0.1.4",
    "tslint": "^4.0.0",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.3.0",
    "uri-templates": "^0.2.0",
